{
  "jsc": {
    "parser": {
      "syntax": "typescript",
      "decorators": true
    },
    "target": "es2023",
    "paths": {
      "@/*": ["./*"]
    },
    "transform": {
      "decoratorMetadata": true,
      "legacyDecorator": true,
      "decoratorVersion": "2021-12"
    },
    "baseUrl": "./src"
  },
  "module": {
    "type": "commonjs",
    "importInterop": "node"
  },
  "sourceMaps": false
}
